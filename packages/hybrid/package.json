{
  "name": "@activeledger/activehybrid",
  "description": "Activeledger Hybrid Connect, Maintain every permissioned chain yourself",
  "version": "2.11.8",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "blockchain",
    "dlt",
    "distributed ledger technology",
    "ledger",
    "smart contracts",
    "activeledger",
    "network",
    "hybrid"
  ],
  "homepage": "https://activeledger.io",
  "preferGlobal": true,
  "bin": {
    "activehybrid": "./lib/index.js"
  },
  "scripts": {
    "start": "node lib/index.js",
    "build:commonjs": "tsc --outDir lib --module commonjs  && copyfiles -f ./src/default.config.json ./lib && copyfiles -u 3 ../activeledger/lib/contracts/default/* ./lib",
    "build:es": "tsc --outDir es --module es6  && copyfiles -f ./src/default.config.json ./es && copyfiles -u 3 ../activeledger/es/contracts/default/* ./es",
    "build": "npm run build:commonjs && npm run build:es",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activeledger/activeledger.git"
  },
  "bugs": {
    "url": "https://github.com/activeledger/activeledger/issues"
  },
  "author": "Activeledger",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "18.0.0",
    "copyfiles": "2.4.1"
  },
  "files": [
    "es",
    "lib"
  ],
  "types": "./lib/index.d.ts",
  "jsnext:main": "./es/index.js",
  "module": "./es/index.js",
  "dependencies": {
    "@activeledger/activecontracts": "^2.11.8",
    "@activeledger/activecrypto": "^2.11.8",
    "@activeledger/activedefinitions": "^2.11.8",
    "@activeledger/activelogger": "^2.7.0",
    "@activeledger/activeoptions": "^2.11.8",
    "@activeledger/activeprotocol": "^2.11.8",
    "@activeledger/activestorage": "^2.11.8",
    "@activeledger/activetoolkits": "^2.11.8",
    "@activeledger/activeutilities": "^2.7.0",
    "@activeledger/httpd": "^2.7.0",
    "typescript": "4.7.3"
  },
  "gitHead": "0e3737c01cf21565ceee635a5b912af3020a5fd6"
}
