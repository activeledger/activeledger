{
  "name": "@activeledger/activeledger",
  "version": "2.0.0-rc.8.0.65",
  "description": "This package contains the main application for running the Activeledger blockchain",
  "keywords": [
    "blockchain",
    "dlt",
    "distributed ledger technology",
    "ledger",
    "smart contracts",
    "activeledger",
    "network",
    "node"
  ],
  "homepage": "https://activeledger.io",
  "preferGlobal": true,
  "bin": {
    "activeledger": "./lib/index.js"
  },
  "scripts": {
    "start": "node lib/index.js",
    "test": "cd ../../ && npm test",
    "build:commonjs": "tsc --outDir lib --module commonjs && copyfiles -f ./src/default.config.json ./lib",
    "build:es": "tsc --outDir es --module es6 && copyfiles -f ./src/default.config.json ./es",
    "build": "npm run build:commonjs && npm run build:es",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activeledger/activeledger.git"
  },
  "bugs": {
    "url": "https://github.com/activeledger/activeledger/issues"
  },
  "author": "Activeledger",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.12.23",
    "copyfiles": "^2.1.0",
    "typescript": "^3.3.3"
  },
  "files": [
    "es",
    "lib"
  ],
  "types": "./lib/index.d.ts",
  "jsnext:main": "./es/index.js",
  "module": "./es/index.js",
  "dependencies": {
    "@activeledger/activecontracts": "^2.0.0-rc.8.0.65",
    "@activeledger/activecrypto": "^2.0.0-rc.8.0.64",
    "@activeledger/activelogger": "^2.0.0-rc.8.0.62",
    "@activeledger/activenetwork": "^2.0.0-rc.8.0.65",
    "@activeledger/activeoptions": "^2.0.0-rc.8.0.64",
    "@activeledger/activestorage": "^2.0.0-rc.8.0.65"
  },
  "optionalDependencies": {
    "@_activeledger/activetoolkits": "^2.0.0-rc.8.0.56"
  }
}
